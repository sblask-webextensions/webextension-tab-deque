{
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "tabdeque@sblask"
        }
    },
    "commands": {
        "select-tab-from-end-of-tabdeque": {
            "description": "Select tab from end of TabDeque",
            "suggested_key": {
                "default": "Ctrl+Up"
            }
        },
        "send-tab-to-end-of-tabdeque": {
            "description": "Send tab to end of TabDeque",
            "suggested_key": {
                "default": "Ctrl+Down"
            }
        }
    },
    "description": "A webextension for better tab handling. Inspired by Opera 12.",
    "homepage_url": "https://github.com/sblask/webextension-tab-deque",
    "icons": {
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "48": "icon-48x48.png",
        "64": "icon-64x64.png",
        "128": "icon-128x128.png"
    },
    "manifest_version": 2,
    "name": "Tab Deque",
    "options_ui": {
        "page": "options/options.html"
    },
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "version": "x.x.x"
}
